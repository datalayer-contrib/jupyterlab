{
  "name": "@jupyterlab/debugger",
  "version": "3.0.0-alpha.12",
  "description": "JupyterLab - Debugger Extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js.map",
    "lib/**/*.js",
    "schema/*.json",
    "style/**/*.css",
    "style/**/*.svg"
  ],
  "sideEffects": [
    "style/**/*.css"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "build:test": "tsc --build tsconfig.test.json",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo && rimraf tsconfig.test.tsbuildinfo && rimraf tests/build",
    "docs": "typedoc --options tdoptions.json --theme ../../typedoc-theme src",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "jest --collect-coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:watch": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0-alpha.12",
    "@jupyterlab/apputils": "^3.0.0-alpha.12",
    "@jupyterlab/cells": "^3.0.0-alpha.12",
    "@jupyterlab/codeeditor": "^3.0.0-alpha.12",
    "@jupyterlab/codemirror": "^3.0.0-alpha.12",
    "@jupyterlab/console": "^3.0.0-alpha.12",
    "@jupyterlab/coreutils": "^5.0.0-alpha.12",
    "@jupyterlab/docregistry": "^3.0.0-alpha.12",
    "@jupyterlab/fileeditor": "^3.0.0-alpha.12",
    "@jupyterlab/notebook": "^3.0.0-alpha.12",
    "@jupyterlab/observables": "^4.0.0-alpha.12",
    "@jupyterlab/services": "^6.0.0-alpha.12",
    "@jupyterlab/translation": "^3.0.0-alpha.12",
    "@jupyterlab/ui-components": "^3.0.0-alpha.12",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/commands": "^1.10.1",
    "@lumino/coreutils": "^1.4.3",
    "@lumino/datagrid": "^0.6.0",
    "@lumino/disposable": "^1.3.5",
    "@lumino/signaling": "^1.3.5",
    "@lumino/widgets": "^1.11.1",
    "codemirror": "~5.56.0",
    "murmurhash-js": "^1.0.0",
    "react": "~16.9.0",
    "vscode-debugprotocol": "^1.37.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@jupyterlab/testutils": "^3.0.0-alpha.12",
    "@types/codemirror": "^0.0.76",
    "@types/jest": "^24.0.23",
    "@types/murmurhash-js": "1.0.3",
    "@types/react-dom": "~16.9.4",
    "canvas": "^2.6.1",
    "jest": "^25.2.3",
    "jest-junit": "^10.0.0",
    "jest-raw-loader": "^1.0.1",
    "jest-summary-reporter": "^0.0.2",
    "rimraf": "~3.0.0",
    "shell-quote": "^1.7.2",
    "ts-jest": "^25.2.1",
    "typedoc": "^0.17.7",
    "typescript": "~3.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
